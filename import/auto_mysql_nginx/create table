CREATE TABLE `Basic_Information` (
	`id` INT(10) NOT NULL AUTO_INCREMENT,
	`ip` CHAR(20) NULL DEFAULT NULL,
	`time` CHAR(50) NULL DEFAULT NULL,
	`date` CHAR(50) NULL DEFAULT NULL,
	`status_code` CHAR(5) NULL DEFAULT NULL,
	`Rresponse_time` CHAR(5) NULL DEFAULT NULL,
	`data_size` CHAR(50) NULL DEFAULT NULL,
	`host` INT(5) NULL DEFAULT NULL,
	PRIMARY KEY (`id`)
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=24
;

CREATE TABLE `Device_Information` (
	`id` INT(10) NOT NULL AUTO_INCREMENT,
	`system` CHAR(50) NULL DEFAULT NULL,
	`version` CHAR(50) NULL DEFAULT NULL,
	`sum` VARCHAR(200) NULL DEFAULT NULL,
	PRIMARY KEY (`id`)
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=24
;

CREATE TABLE `host_list` (
	`host_name` CHAR(50) NULL DEFAULT NULL,
	`host` INT(5) UNSIGNED ZEROFILL NOT NULL AUTO_INCREMENT,
	INDEX `Index 1` (`host`)
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=3
;

CREATE TABLE `User_request` (
	`id` INT(10) NOT NULL AUTO_INCREMENT,
	`request_type` CHAR(100) NULL DEFAULT NULL,
	`request_resource` CHAR(100) NULL DEFAULT NULL,
	PRIMARY KEY (`id`)
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=24
;
